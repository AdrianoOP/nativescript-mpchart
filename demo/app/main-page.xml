<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" class="page"
  xmlns:chart="nativescript-mpchart">
  <Page.actionBar>
      <ActionBar title="Range Seek Bar"></ActionBar>
  </Page.actionBar>
  <StackLayout height="500">
      <chart:MPLineChart height="300" items="{{ dataSet }}" labels="{{ labels }}" class="mp-chart"/>
  </StackLayout>
</Page>